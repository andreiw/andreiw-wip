+#define PDE_PRESENT(pde)	(pde_val(pde) & PDE_TYPE_MASK)
+#define PDE_CLEAR(pde)		(pde_val(*(pde)) = 0UL)
+
+typedef struct { unsigned long pte; } pte_t;
+typedef struct { unsigned long pgd; } pde_t;
+
+#endif /* __ASSEMBLY__ */
+
+#endif /* _ASMARM_PGTABLE_H */

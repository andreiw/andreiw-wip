+
+#define __put_user_asm_byte(x,__pu_addr,err)				\
+	__asm__ __volatile__(						\
+	"1:	strbt	%1,[%2],#0\n"					\
+	"2:\n"								\
+	"	.section .fixup,\"ax\"\n"				\
+	"	.align	2\n"						\
+	"3:	mov	%0, %3\n"					\

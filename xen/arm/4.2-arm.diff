+#ifndef __ARM_CONFIG_H_
+#define __ARM_CONFIG_H_
+
+#ifdef CONFIG_USE_HIGH_VECTORS
+#define VECTORS_BASE	0xFFFF0000
+#else
+#define VECTORS_BASE	0x00000000
+#endif
+
+#ifndef NDEBUG
+#define MEMORY_GUARD
+#endif
+
